<h1>All Calves</h1>
<table>
  <tr><th>Calf ID</th><th>Birth Date</th><th>Age (Days)</th></tr>
  <% calves.forEach(calf => { %>
    <tr>
      <td><a href="/calves/<%= calf._id %>"><%= calf.calfId %></a></td>
      <td><%= calf.birthDate.toDateString() %></td>
      <td><%= Math.floor((Date.now() - new Date(calf.birthDate)) / (1000*60*60*24)) %></td>
    </tr>
  <% }) %>
</table>
